{
  "name": "kusama-js",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "standard_trade": "npm run build && node dist/standard_trade.js",
    "buyer_cancels": "npm run build && node dist/buyer_cancels.js",
    "seller_cancels": "npm run build && node dist/seller_cancels.js",
    "buyer_disputes_wins": "npm run build && node dist/buyer_disputes_wins.js",
    "buyer_disputes_loses": "npm run build && node dist/buyer_disputes_loses.js",
    "seller_disputes_wins": "npm run build && node dist/seller_disputes_wins.js",
    "seller_disputes_loses": "npm run build && node dist/seller_disputes_loses.js"
  },
  "license": "MIT",
  "dependencies": {
    "@polkadot/api": "^1.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^3.0.2"
  }
}
